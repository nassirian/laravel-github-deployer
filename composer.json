{
  "name": "nassirian/laravel-github-deployer",
  "description": "A simple GitHub webhook-based deployer for Laravel applications.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Nassirian\\GitHubDeployer\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8|^9|^10"
  },
  "autoload-dev": {
    "psr-4": {
      "Nassirian\\GitHubDeployer\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
    "mockery/mockery": "^1.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nassirian\\GitHubDeployer\\GitHubDeployerServiceProvider"
      ]
    }
  }
}
